{
  "AWSEBDockerrunVersion": 2,

  "containerDefinitions": [

    {
      "name": "jing-api",
      "image": "jingc12/jing-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://db:5432/customer"
        }
      ]
    }
  ]
}